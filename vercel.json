{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "headers": [
      {
        "source": "/public/imagens/onePiece2.mp4",
        "headers": [
          {
            "key": "Content-Type",
            "value": "video/mp4"
          }
        ]
      }
    ]
  }
  